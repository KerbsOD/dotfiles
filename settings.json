{
    "window.titleBarStyle": "custom",
    "editor.tabSize": 8,       
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.fontSize": 15,
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'monospace', monospace",
    "editor.fontWeight": "400",
    "editor.bracketPairColorization.enabled": false,
    "editor.inlayHints.enabled": "off",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "editor.tokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "textMateRules": [
                {
                        "scope": [
                                "variable",
                        ],
                        "settings": {
                            "foreground": "#EEFFFF",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                        ],
                        "settings": {
                            "foreground": "#CAC8CB",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                        ],
                        "settings": {
                            "foreground": "#1E1E1E",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                        ],
                        "settings": {
                            "foreground": "#FF5242",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                        ],
                        "settings": {
                            "foreground": "#82AAFF",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                                "keyword.operator"
                        ],
                        "settings": {
                            "foreground": "#89DDFF",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                                "support.type",
                                "entity.name.type",
                                "entity.name.import",
                        ],
                        "settings": {
                            "foreground": "#C3E88D",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                                "entity.name.namespace",
                        ],
                        "settings": {
                            "foreground": "#F9CB5E",
                            "fontStyle": "",
                        }
                },
                {
                        "scope": [
                                "keyword",
                                "keyword.control",
                                "support.constant"
                        ],
                        "settings": {
                                "foreground": "#C792EA",
                                "fontStyle": "",
                        }
                },
                {
                        "scope": [
                                "variable.parameter",
                                "variable.other.constant",
                        ],
                        "settings": {
                            "foreground": "#F78C6C",
                            "fontStyle": "",
                        }
                },
            ],
        }
    },
    "editor.lineNumbers": "relative",
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "gopls": {
        "ui.semanticTokens": true
    }

}